int mode = 0; // 0 - Splash, 1 - About, 2 - Fortune, 3 - Mood, 4 - Help
int lastMode = 0;

void setup(void) {
  Serial.begin(9600);

  tft.initR(INITR_BLACKTAB);
  tft.fillScreen(MAGENTA);

  pinMode(pos1, INPUT);
  pinMode(pos2, INPUT);
  pinMode(pos3, INPUT);
  pinMode(pos4, INPUT);

  delay(1000);
}

void loop() {
  switch (mode) {
    case 0: showSplash(); break;
    case 1: showAbout(); break;
    case 2: showFortune(); break;
    case 3: showMood(); break;
    case 4: showHelp(); break;
  }
  mode = currentMode();
  if (lastMode != mode) {
    tft.fillScreen(MAGENTA);
    lastMode = mode;
    isAbout = false;
    isHelp = false;
    isMood = false;
    isFortune = false;
    isResult = false;
    moodMode = 0;
    fortuneMode = 0;
  }
}

void showSplash() {
  tft.drawBitmap(0, 0, splash, 128, 160, WHITE);
  delay(2000);
  mode = currentMode();
}

